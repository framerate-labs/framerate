{
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "importOrder": [
    "<TYPES>^(node:)",
    "<TYPES>",
    "<TYPES>^[.]",
    "",
    "^react$|react-dom$",
    "@tanstack",
    "@clerk",
    "<THIRD_PARTY_MODULES>",
    "",
    "^(@api|@assets|@ui)(/.*)$",
    "@/store",
    "@/components",
    "@/features",
    "@/lib",
    "@/utils",
    "",
    "@/styles",
    "^(?!.*[.]css$).*$",
    ".css$",
    "",
    "^[.]",
    "/src*"
  ]
}
