{
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "importOrder": [
    "<TYPES>^(node:)",
    "<TYPES>",
    "<TYPES>^[.]",
    "",
    "^react$|react-dom$",
    "@tanstack",
    "@elysiajs",
    "<THIRD_PARTY_MODULES>",
    "",
    "^(@api|@assets|@ui)(/.*)$",
    "@server",
    "@web",
    "",
    "@web/styles",
    "^(?!.*[.]css$).*$",
    ".css$",
    "",
    "^[.]",
    "/src*"
  ]
}
